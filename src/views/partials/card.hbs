
  {{#each objeto}}
    

<div class="card card-1" style="width: 18rem;">
  <img class="card-img-top" src="data:image/png;base64," alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.nombre}}, {{this.apellidos}}</h5>
    <p class="card-text">{{this.dni}}</p>
    <p class="card-text">{{this.edad}}</p>
   
  </div>
</div>

  {{/each}}